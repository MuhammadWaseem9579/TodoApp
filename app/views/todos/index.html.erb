<h1>Here are all Todos</h1>
<%= link_to "Create new Todo", new_todo_path %>
<table border="1px solid black">
	<th>Name</th>
	<th colspan="4">Description</th>
	<% @todos.each do |todo| %>
		<tr>
			<td><%= todo.name %></td>
			<td><%= todo.discription %></td>
			<td><%= link_to "Edit", edit_todo_path(todo) %></td>
			<td><%= link_to "Show", todo_path(todo) %></td>
			<td><%= link_to "Delte", todo_path(todo), method: :delete , data: {confirm:"Are you sure?"}%></td>
		</tr>
	<% end %>
</table>